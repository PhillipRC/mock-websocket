{
  "name": "mock-websocket",
  "version": "0.0.4",
  "description": "Javascript mocking library for websockets",
  "main": "./dist/mock-websocket",
  "types": "./mock-websocket.d.ts",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && rollup -c rollup.config.js",
    "lint": "eslint src tests",
    "js:check": "tsc -p tsconfig.json",
    "js:lint": "tslint -p tsconfig.json",
    "js:test": "jest",
    "prettier": "prettier --write '{src,tests}/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vovkasm/mock-websocket.git"
  },
  "keywords": [
    "websocket",
    "websockets",
    "mock"
  ],
  "author": "Vladimir Timofeev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vovkasm/mock-websocket/issues"
  },
  "homepage": "https://github.com/vovkasm/mock-websocket",
  "dependencies": {
    "url-parse": "^1.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/url-parse": "^1.4.3",
    "@vovkasm/tslint-config": "^2.0.1",
    "babel-jest": "^24.5.0",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "rollup": "^1.7.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "systemjs": "^3.0.2",
    "ts-jest": "^24.0.0",
    "tslib": "^1.9.3",
    "tslint": "^5.14.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.4000"
  }
}
