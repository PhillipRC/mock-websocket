{
  "name": "mock-websocket",
  "version": "0.0.1",
  "description": "Javascript mocking library for websockets",
  "main": "./dist/mock-socket",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test ava --serial --verbose",
    "test:coverage": "cross-env NODE_ENV=test nyc ava --serial",
    "build": "rm -rf dist && rollup -c rollup.config.js",
    "lint": "eslint src tests",
    "format": "prettier --print-width 120 --single-quote --write '{src,tests}/**/*.js'",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vovkasm/mock-websocket.git"
  },
  "keywords": [
    "websockets",
    "mock",
    "mocksocket"
  ],
  "author": "Vladimir Timofeev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vovkasm/mock-websocket/issues"
  },
  "homepage": "https://github.com/vovkasm/mock-websocket",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.0.1",
    "jsdom": "^11.5.1",
    "nyc": "^10.3.2",
    "prettier": "^1.3.1",
    "rollup": "^0.55.1",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "systemjs": "^0.20.12"
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "url-parse": "^1.2.0"
  }
}
